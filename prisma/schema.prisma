generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user {
  id      Int     @id @default(autoincrement())
  name    String?
  age     Int
  address String?
  gender  Gender?  @default(Unknown)
}

model profile {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
}

enum Gender{
  Male
  Female
  Unknown
}